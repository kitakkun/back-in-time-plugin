import com.github.kitakkun.backintime.websocket.event.BackInTimeDebugServiceEvent;

CREATE TABLE IF NOT EXISTS eventLog(
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    sessionId TEXT NOT NULL,
    payload TEXT AS BackInTimeDebugServiceEvent NOT NULL,
    createdAt INTEGER NOT NULL
);

selectSessionEvents:
SELECT * FROM eventLog WHERE sessionId = ?;

insert:
INSERT INTO eventLog(sessionId, payload, createdAt) VALUES(?, ?, ?);

deleteSessionEvents:
DELETE FROM eventLog WHERE sessionId = ?;
